{
  "name": "starter-web",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3100",
    "build": "next build",
    "start": "next start",
    "lint": "npx tsc --noEmit && next lint --fix",
    "prepare": "husky install",
    "generate": "eval $(egrep -v '^#' .env | xargs) graphql-codegen --config codegen.yml",
    "dev:generate": "eval $(egrep -v '^#' .env | xargs) graphql-codegen --config codegen.yml --watch",
    "generate-vercel": "graphql-codegen --config codegen.yml",
    "update-template": "update-template https://github.com/flexper/starter-web"
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint --fix"
  },
  "dependencies": {
    "@headlessui/react": "1.7.16",
    "@hookform/resolvers": "3.1.1",
    "@t3-oss/env-nextjs": "^0.6.0",
    "@tailwindcss/forms": "0.5.4",
    "@tanstack/react-query": "4.32.6",
    "@tanstack/react-query-devtools": "4.32.6",
    "clsx": "2.0.0",
    "graphql": "16.7.1",
    "next": "13.4.12",
    "next-protected-auth": "2.0.185",
    "next-seo": "6.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.45.2",
    "react-hot-toast": "2.4.1",
    "react-select": "5.7.4",
    "rosetty-react": "1.4.180",
    "socket.io-client": "4.7.2",
    "tailwindcss": "3.3.3",
    "usehooks-ts": "2.9.1",
    "zod": "^3.21.4",
    "zod-rosetty": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.22.9",
    "@commitlint/cli": "17.6.7",
    "@commitlint/config-conventional": "17.6.7",
    "@graphql-codegen/cli": "5.0.0",
    "@graphql-codegen/typescript": "4.0.1",
    "@graphql-codegen/typescript-operations": "4.0.1",
    "@graphql-codegen/typescript-react-query": "4.1.0",
    "@hookform/devtools": "4.3.1",
    "@tailwindcss/line-clamp": "0.4.4",
    "@types/node": "18.17.2",
    "@types/react": "18.2.18",
    "@types/react-dom": "18.2.7",
    "@typescript-eslint/eslint-plugin": "6.2.1",
    "@typescript-eslint/parser": "6.2.1",
    "autoprefixer": "10.4.14",
    "csstype": "3.1.2",
    "eslint": "8.46.0",
    "eslint-config-next": "13.4.12",
    "eslint-config-prettier": "8.10.0",
    "eslint-plugin-import": "2.28.0",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "husky": "8.0.3",
    "lint-staged": "13.2.3",
    "postcss": "8.4.27",
    "prettier": "3.0.1",
    "prettier-plugin-tailwindcss": "0.4.1",
    "sass": "1.64.2",
    "ts-node": "10.9.1",
    "typescript": "5.1.6",
    "update-template": "1.3.1"
  }
}
