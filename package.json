{
  "name": "starter-web",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3100",
    "build": "next build",
    "start": "next start",
    "lint": "npx tsc --noEmit && next lint --fix",
    "prepare": "husky install",
    "generate": "eval $(egrep -v '^#' .env | xargs) graphql-codegen --config codegen.yml",
    "dev:generate": "eval $(egrep -v '^#' .env | xargs) graphql-codegen --config codegen.yml --watch",
    "generate-vercel": "graphql-codegen --config codegen.yml",
    "update-template": "update-template https://github.com/flexper/starter-web"
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint --fix"
  },
  "dependencies": {
    "@headlessui/react": "1.7.12",
    "@hookform/resolvers": "2.9.11",
    "@tailwindcss/forms": "0.5.3",
    "@tanstack/react-query": "4.24.10",
    "@tanstack/react-query-devtools": "4.24.12",
    "clsx": "1.2.1",
    "env-vars-validator": "1.2.5",
    "graphql": "16.6.0",
    "next": "13.2.1",
    "next-protected-auth": "2.0.93",
    "next-seo": "5.15.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.43.2",
    "react-hot-toast": "2.4.0",
    "react-select": "5.7.0",
    "rosetty-react": "1.4.86",
    "socket.io-client": "4.6.1",
    "tailwindcss": "3.2.7",
    "usehooks-ts": "2.9.1"
  },
  "devDependencies": {
    "@babel/core": "7.21.0",
    "@commitlint/cli": "17.4.4",
    "@commitlint/config-conventional": "17.4.4",
    "@graphql-codegen/cli": "3.2.1",
    "@graphql-codegen/typescript": "3.0.1",
    "@graphql-codegen/typescript-operations": "3.0.1",
    "@graphql-codegen/typescript-react-query": "4.1.0",
    "@hookform/devtools": "4.3.0",
    "@tailwindcss/line-clamp": "0.4.2",
    "@types/node": "18.14.2",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@typescript-eslint/eslint-plugin": "5.54.0",
    "@typescript-eslint/parser": "5.54.0",
    "autoprefixer": "10.4.13",
    "csstype": "3.1.1",
    "eslint": "8.35.0",
    "eslint-config-next": "13.2.1",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "husky": "8.0.3",
    "lint-staged": "13.1.2",
    "postcss": "8.4.21",
    "prettier": "2.8.4",
    "prettier-plugin-tailwindcss": "0.2.3",
    "sass": "1.58.3",
    "ts-node": "10.9.1",
    "typescript": "4.9.5",
    "update-template": "1.3.1"
  }
}
