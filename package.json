{
  "name": "starter-web",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3100",
    "build": "next build",
    "start": "next start",
    "lint": "npx tsc --noEmit && next lint --fix",
    "prepare": "husky install",
    "generate": "eval $(egrep -v '^#' .env | xargs) graphql-codegen --config codegen.yml",
    "dev:generate": "eval $(egrep -v '^#' .env | xargs) graphql-codegen --config codegen.yml --watch",
    "generate-vercel": "graphql-codegen --config codegen.yml",
    "update-template": "update-template https://github.com/flexper/starter-web"
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint --fix"
  },
  "dependencies": {
    "@headlessui/react": "1.7.3",
    "@hookform/resolvers": "2.9.10",
    "@tailwindcss/forms": "0.5.3",
    "@tanstack/react-query": "4.13.0",
    "@tanstack/react-query-devtools": "4.13.0",
    "clsx": "1.2.1",
    "env-vars-validator": "1.1.116",
    "graphql": "16.6.0",
    "next": "12.3.1",
    "next-protected-auth": "2.0.21",
    "next-seo": "5.11.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.38.0",
    "react-hot-toast": "2.4.0",
    "react-select": "5.5.7",
    "rosetty-react": "1.4.21",
    "socket.io-client": "4.5.3",
    "tailwindcss": "3.2.1",
    "usehooks-ts": "2.9.1"
  },
  "devDependencies": {
    "@babel/core": "7.19.6",
    "@commitlint/cli": "17.1.2",
    "@commitlint/config-conventional": "17.1.0",
    "@graphql-codegen/cli": "2.13.8",
    "@graphql-codegen/typescript": "2.8.0",
    "@graphql-codegen/typescript-operations": "2.5.5",
    "@graphql-codegen/typescript-react-query": "4.0.3",
    "@hookform/devtools": "4.2.2",
    "@tailwindcss/line-clamp": "0.4.2",
    "@types/node": "18.11.7",
    "@types/react": "18.0.24",
    "@types/react-dom": "18.0.8",
    "@typescript-eslint/eslint-plugin": "5.41.0",
    "@typescript-eslint/parser": "5.41.0",
    "autoprefixer": "10.4.13",
    "csstype": "3.1.1",
    "eslint": "8.26.0",
    "eslint-config-next": "12.3.1",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-simple-import-sort": "8.0.0",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "postcss": "8.4.18",
    "prettier": "2.7.1",
    "prettier-plugin-tailwindcss": "0.1.13",
    "sass": "1.55.0",
    "ts-node": "10.9.1",
    "typescript": "4.8.4",
    "update-template": "1.3.1"
  }
}
